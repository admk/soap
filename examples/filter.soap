input (
    a0: [0.2, 0.3][0, 0], a1: [0.1, 0.2][0, 0], a2: [0.0, 0.1][0, 0],
    b0: [0.2, 0.3][0, 0], b1: [0.1, 0.2][0, 0], b2: [0.0, 0.1][0, 0],
    x: [1.0, 1.0][0, 0],
    n: 20
);
output (y);

x1 := 0.0; x2 := 0.0;
y1 := 0.0; y2 := 0.0;
y := x;
i := 0;
while (i < n) (
    i := i + 1;
    y := b0 * x + b1 * x1 + b2 * x2 + a0 * y + a1 * y1 + a2 * y2;
    x2 := x1; x1 := x;
    y2 := y1; y1 := y;
);
