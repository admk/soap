# original
input (n: 20, x: [-0.1, 0.1][0, 0], y: [0, 1][0, 0]);
output (z);
a := 1;
i := 0; j := 0; z := 0;
p := 1; s := x + y;
while (i < n) (
    i := i + 1;
    p := p * s;
    a := -a;
    b := 1;
    j := 0;
    while (j < 2 * i + 1) (
        j := j + 1;
        b := b * j;
    );
    z := z + (a / b) * p;
);
