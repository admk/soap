input (n: 20, b1: [-0.1, 0.1][0, 0], b2: [-1, 1][0, 0]);
output (x1, x2);

a11 := 2.0; a12 := 1.0; a21 := 1.0; a22 := 2.0;
ra11 := 1.0 / a11;
ra22 := 1.0 / a22;

i := 0;
x1 := 0; x2 := 0; x1t := 0;

while (i < n) (
    i := i + 1;
    x1t := (b1 - a12 * x2) * ra11;
    x2 := (b2 - a21 * x1) * ra22;
    x1 := x1t;
);
