input (
    n: [10, 20],
    a11: [1.0, 200.0][0, 0],
    a12: [-0.9, 0.1][0, 0],
    a22: [1.0, 2.0][0, 0],
    a21: [-0.1, 0.9][0, 0],
    b1: [-100, 0][0, 0],
    b2: [100, 200][0, 0]
);
output (x1, x2);

i := 0;
x1 := 0.0; x2 := 0.0;

while (i < n) (
    i := i + 1;
    x1t := (b1 - a12 * x2) / a11;
    x2 := (b2 - a21 * x1) / a22;
    x1 := x1t;
);
